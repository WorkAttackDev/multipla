{
  "name": "splinx-api",
  "version": "1.0.0",
  "description": "API for splinx system",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.ts",
    "build": "cross-env NODE_ENV=production npx tsc",
    "start": "cross-env NODE_ENV=production node build/index.js >> app.log",
    "migrate:up": "npx knex migrate:latest  --knexfile src/config/db/knexfile.ts",
    "migrate:new": "npx knex migrate:make init --knexfile src/config/db/knexfile.ts",
    "migrate:down": "npx knex migrate:down --knexfile src/config/db/knexfile.ts",
    "migrate:prod:up": "npx knex migrate:latest  --knexfile build/src/config/db/knexfile.js",
    "migrate:prod:down": "npx knex migrate:down  --knexfile build/src/config/db/knexfile.js"
  },
  "author": "Denilson Costa",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "knex": "^0.95.11",
    "mysql": "^2.18.1",
    "splynx-nodejs-api": "^1.0.8",
    "zod": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.10",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  }
}